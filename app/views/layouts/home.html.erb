<script>
    $("#home_search").click(function(){
        if($('#phone_number').val() !== undefined || $('#phone_number').val() !== ""){
            window.location.href='/home/' + $('#phone_number').val()
        }else{
            alert('Утасны дугаараа оруулна уу!')
        }
    });
</script>
